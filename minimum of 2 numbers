#include<stdio.h>
int input(int *a,int *b)
	{
	printf("enter two numbers:");
	scanf("%d%d",a,b);
	}
int compare(int x,int y)
	{
	 if(x<y)
	 {
	 printf("%d is the smaller number\n",x);
	 return x;
	 }
	 else
	 {
	 printf("%d is the smaller number\n",y);
         return y;
	 }
	}
int output(int z)
	{
	printf("%d is the smaller number among the given two numbers:",z);
	}
int main()
	{
	int p,q,r,s;
	p=input(&q,&r);
	r=compare(q,r);
	output(r);
}
	
