#include<stdio.h>
int input()
  {
  int a,b;
  printf("enter two numbers:");
  scanf("%d%d",&a,&b);
  return 0;
  }
int compare(int z)
  {
  int x,y;
  if(x<y)
    {
    printf("the smallest number is %d",x);
    return x;
    }
  else
    {
    printf("the smallest number is %d",y);
    return y;
    }
 }
int output(int c)
  {
  printf("the smallest number among the given two numbers is:",c);
  return c;
  }
int main()
  {
  int p,q;
  p=input();
  q=compare(p);
  output(q);
  }
